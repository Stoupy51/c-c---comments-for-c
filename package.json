{
  "name": "c-c---comments-for-c",
  "displayName": "C/C++ comments for C#",
  "description": "Bring back C/C++ multi-line comments colors for C#",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.76.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "c#",
      "aliases": ["C#", "c#"],
      "extensions": [".cs"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "c#",
      "scopeName": "source.csharp",
      "path": "./syntaxes/c#.tmLanguage.json"
    }]
  }
}
